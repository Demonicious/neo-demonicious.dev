  "author": "Ross Hill <ross@rosshill.ca> (https://rosshill.ca)",
  "type": "module",
  "scripts": {
    "prebuild": "npm run writeNodeVersion && npm run getPosts && npm run getIcons && npm run getWebp",
    "build": "svelte-kit build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "svelte-kit dev",
    "getIcons": "node --loader ts-node/esm ./scripts/getIcons.ts",
    "getPosts": "markdown-json -c content/.mdconfig.json",
    "getWebp": "node --loader ts-node/esm ./scripts/getWebp.ts",
    "lint": "eslint . --ignore-path .gitignore",
    "serve": "sirv build -p 3000",
    "pretest": "rm -rf tests/build && rm -rf tests/test-results && tsc --incremental -p tests/tsconfig.json",
    "test": "cd tests && playwright test",
    "upgrade": "ncu -u && ncu -u --target newest --filter \"@sveltejs/adapter-static @sveltejs/kit\" && npm i",
    "writeNodeVersion": "node --loader ts-node/esm ./scripts/writeNodeVersion.ts"
  },
  "browserslist": "last 5 years or ie>=11 or >0.2%",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@iconify/json": "^2.1.31",
    "@playwright/test": "^1.21.1",
    "@sveltejs/adapter-static": "1.0.0-next.29",
    "@sveltejs/kit": "1.0.0-next.320",
    "@types/chroma-js": "^2.1.3",
    "@types/cookie": "^0.5.1",
    "@types/cssnano": "^5.0.0",
    "@types/dateformat": "^5.0.0",
    "@types/imagemin": "^8.0.0",
    "@types/imagemin-webp": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "autoprefixer": "^10.4.4",
    "chroma-js": "^2.4.2",
    "cssnano": "^5.1.7",
    "date-fns": "^2.28.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "hsluv-sass": "^2.0.1",
    "hsluv-ts": "^1.0.1",
    "image-size": "^1.0.1",
    "imagemin": "^8.0.1",
    "imagemin-webp": "^7.0.0",
    "isbot": "^3.4.6",
    "markdown-json": "^1.6.1",
    "sass": "^1.50.1",
    "simple-icons": "^6.19.0",
    "sirv-cli": "^2.0.2",
    "slugify": "^1.6.5",
    "svelte": "3.47.0",
    "svelte-check": "^2.7.0",
    "svelte-meta-tags": "^2.5.1",
    "svelte-preprocess": "^4.10.6",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3",
    "unplugin-icons": "^0.14.1",
    "vite": "2.9.5"
  },
  "engines": {
    "node": ">= 16.13.2"
  }
}
