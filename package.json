  "author": "Ross Hill <ross@rosshill.ca> (https://rosshill.ca)",
  "type": "module",
  "scripts": {
    "prebuild": "npm run writeNodeVersion && npm run getPosts && npm run getIcons && npm run getWebp",
    "build": "svelte-kit build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "svelte-kit dev",
    "export": "npm run build",
    "getIcons": "node --loader ts-node/esm ./getIcons.ts",
    "getPosts": "markdown-json -c markdown-config.json",
    "getWebp": "cd src/assets && webpc -r && cd ..",
    "lint": "eslint . --ignore-path .gitignore",
    "preview": "svelte-kit preview",
    "start": "svelte-kit start",
    "pretest": "tsc --incremental -p tests/integration/tsconfig.json",
    "test": "cd tests/integration && playwright test",
    "upgrade": "ncu -u && ncu -u --target newest --filter \"@sveltejs/adapter-static @sveltejs/kit\" && npm i",
    "writeNodeVersion": "node --loader ts-node/esm ./writeNodeVersion.ts"
  },
  "browserslist": "last 5 years or ie>=11 or >0.2%",
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/eslint-parser": "^7.17.0",
    "@iconify/json": "^2.0.36",
    "@playwright/test": "^1.19.0",
    "@sveltejs/adapter-static": "1.0.0-next.28",
    "@sveltejs/kit": "1.0.0-next.266",
    "@types/chroma-js": "^2.1.3",
    "@types/cookie": "^0.4.1",
    "@types/dateformat": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.4.2",
    "chroma-js": "^2.4.2",
    "cssnano": "^5.0.17",
    "date-fns": "^2.28.0",
    "eslint": "^8.9.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "hsluv-sass": "^2.0.1",
    "hsluv-ts": "^1.0.1",
    "isbot": "^3.4.2",
    "markdown-json": "^1.6.1",
    "sass": "^1.49.7",
    "simple-icons": "^6.9.0",
    "slugify": "^1.6.5",
    "svelte": "3.46.4",
    "svelte-check": "^2.4.3",
    "svelte-elements": "^0.4.2",
    "svelte-preprocess": "^4.10.3",
    "ts-node": "^10.5.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "unplugin-icons": "^0.13.1",
    "vite": "2.8.1",
    "webp-converter-cli": "^0.0.5"
  },
  "engines": {
    "node": ">= 16.13.2"
  }
}
